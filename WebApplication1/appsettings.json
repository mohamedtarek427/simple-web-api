{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "IncludeScopes": true
    },
    "File": {
      "LogLevel": "Information",
      "Path": "Logs/app.log",
      "MinimumLevel": "Information"
    }
  },
  "AllowedHosts": "*"
}
